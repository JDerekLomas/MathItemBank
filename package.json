{
  "name": "math-item-bank",
  "version": "1.0.0",
  "description": "Comprehensive K12 math item bank generator based on educational standards",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/examples/sample-generation.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "generate": "ts-node src/examples/sample-generation.ts",
    "generate:enhanced": "ts-node src/examples/enhanced-sample-generation.ts",
    "claude:setup": "ts-node src/scripts/claude-setup.ts",
    "claude:test": "ts-node src/scripts/claude-test.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "math",
    "education",
    "assessment",
    "item-bank",
    "k12",
    "standards",
    "test-generation",
    "typescript"
  ],
  "author": "MathItemBank Generator",
  "license": "MIT",
  "dependencies": {
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/math-item-bank.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/math-item-bank/issues"
  },
  "homepage": "https://github.com/your-username/math-item-bank#readme"
}